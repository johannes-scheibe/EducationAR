<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.educationar.ui.camera.CameraFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_marker"
        android:name="com.example.educationar.ui.marker.MarkerFragment"
        android:label="@string/title_marker"
        tools:layout="@layout/fragment_marker" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.educationar.ui.notifications.NotificationsFragment"
        android:label="@string/title_add_marker"
        tools:layout="@layout/fragment_add_model" />
</navigation>
